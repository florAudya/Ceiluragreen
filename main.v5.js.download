      this.currentMaterialView.css({ transform: 'translate(' + this.initialPosition.left/10 + 'rem, ' + this.initialPosition.top/10 + 'rem)', position: 'absolute', top: 0, left: 0 });
      this.$('.next-material').addClass('no-animate');

