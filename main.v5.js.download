    initialize: function(options) {
      this.rewards = options.rewards;
      this.customRewards = options.customRewards;
      this.render();
    },
    render: function() {
      // this.chooseOptions();
    },
    chooseOptions: function() {
      preloadImages('reward-animations-level-' + currentLevel, _.map(_.compact(_.flatten(_.pluck(this.rewards[currentLevel], 'preloadImages'))), function(url){ return recollect.baseURL + url; }));
      preloadImages('custom-reward-animations-level-' + currentLevel, _.map(_.compact(_.flatten(_.pluck(this.customRewards[currentLevel], 'preloadImages'))), function(url){ return recollect.baseURL + url; }));
      this.$('.reward-placed .latest').removeClass('latest');
      this.$('.reward-options .slot').remove();
      letThereBePeople();
      // this.selected = null;
      var options = _.sample(this.customRewards[currentLevel], this.maxObjects);
      options = _.shuffle(options.concat(_.sample(this.rewards[currentLevel], this.maxObjects - options.length)));

      for (var i = 0; i < options.length; i++) {
        this.$('.reward-options').append(this.objectTemplate(options[i]));
      }
      // Game is over.
      if ($('.level:not(.done)').length <= 0) {
        this.$('.continue').addClass('hide');
        this.$('.finish').removeClass('hide');
      }
      else {
        if (!this.$('.reward-options').hasClass('place-help')) {
          this.$('.continue').removeClass('hide');
        }
        this.$('.finish').addClass('hide');
      }
      this.slots = $('.slot');
    },
    choose: function(event) { 
      if (debugEnv) {
        this.$('.reward-options h1').html('choose: ' + event.type);
      }
      if (event.type === 'touchstart' || (event.type === 'mousedown' && event.which === 1) || event.type == 'click') {
        this.$('.reward-placed .latest').remove();
        this.$('.reward-options .reward').show();
        var that = this;

        if ($('.level:not(.done)').length > 0) {
          // Only need to remove first time, but run just in case.
          setTimeout(function() {
            that.$('.continue').removeClass('hide');
            that.slots = null;
          }, 1000);
        }

        if(event.type == 'click'){ // keyboard used to select reward
          var currentTarget =  $(event.currentTarget);
          if(currentTarget.hasClass('slot')){ 
            currentTarget = currentTarget.find('.reward');
          }
          this.selected = currentTarget;
          // green area range:
          var top = (Math.random() * (3.0 - 2.8) + 2.8);
          var left = (Math.random() * 8);
          const button = this.selected.parent('.slot');
          button.append(this.selected.clone().hide());
          button.attr('tabindex', '-1');
          // all sibling buttons should get tabindex 0 
          button.siblings().attr('tabindex', '0');


          this.selected.css({
            position: 'absolute',
            top: 0, left: 0,
            transform: 'translate(' + left + 'rem,'+ top + 'rem)'
          });
          // preprend current aria label with "Item placed: "
          this.selected.attr('aria-label', 'Reward placed: ' + this.selected.attr('aria-label'));  
          // announceChange("Item placed");
          // change h1 to say "Move reward with Arrow Keys and hit Enter to place in park"
          this.$('.reward-options h1').html('Move reward with Arrow Keys');
        } else {
          
          this.selected = $(event.currentTarget);
          this.selected.parent('.slot').append(this.selected.clone().hide());
          // this.selected.attr('tabindex', '0');
          this.selected.css({
            position: 'absolute',
            top: 0, left: 0,
          });
          this.selectedOffset.left = -1*($(window).width() - $(this.el).width())/2; // Account for letterboxing in short browsers.
          this.selectedOffset.top = -1*this.selected.height()/2;
          // this.selected.focus();
        }

        var slot = this.selected.parent('.slot');
        var slotIndex = slot.parent().children('.slot').index(slot);
        if(document.getElementById("mute").getAttribute('aria-pressed') == "false"){
          rewardSound[slotIndex].play();
        }

        this.$('.reward-placed').append(this.selected.addClass('latest').attr('tabindex', '0'));
        this.selected.focus();

        if(event.type !== 'click'){
          this.move(event);
        } else {
          this.selected = null;
        }
      }
      this.focusedIdx = null;
    },
    pickup: function(event) {
      var pageX = event.type === 'touchstart' ? event.originalEvent.touches[0].pageX : event.originalEvent.pageX;
      var pageY = event.type === 'touchstart' ? event.originalEvent.touches[0].pageY : event.originalEvent.pageY;
      if (debugEnv) {
        this.$('.reward-options h1').html('pickup: ' + event.type + ': ' + pageX + ', ' + pageY);
      }
      if (event.type === 'touchstart' || (event.type === 'mousedown' && event.which === 1)) {
        this.selected = $(event.currentTarget);
        this.selectedOffset.left = this.selected.position().left + this.selected.width()/2 - pageX;
        this.selectedOffset.top = this.selected.position().top + this.selected.height()/2 - pageY;
        this.move(event);
        this.$('.reward-placed').append(this.selected); // Pop to the top.
        if (this.secondClick) {
          this.selected.toggleClass('flip');
          var stageWidth = $(this.el).width()/100;
          var left = pageX - this.selected.width()/2;
          var top = pageY - this.selected.height()/2;
          this.selected.css({ transform: 'translate(' + (left/stageWidth)/10 + 'rem, ' + (top/stageWidth)/10 + 'rem)' + (this.selected.hasClass('flip') ? ' scaleX(-1)' : '') });
        }
        this.secondClick = true;
        var that = this;
        setTimeout(function() {
          that.secondClick = false;
        }, 500);
        return false;
      }
