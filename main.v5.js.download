    handleHeadingFocusOut: function(event) {
      $('.reward-options').removeClass('place-help');
    },
    changeFocus : function(index) {
      this.focusedButton = this.slots[index];
      this.focusedButton.focus();
    },
    handleKeyPress: function(event){
      // get currently focused element
      const focusedElement = document.activeElement;
      const isFocusedOnReward = focusedElement.classList.contains('reward');

      var VK_LEFT       = 37;
      var VK_UP         = 38;
      var VK_RIGHT      = 39;
      var VK_DOWN       = 40;

      // if this.selected is null, then we are using keyboard to select a slot
      // otherwise, we are placing an item with the keyboard
      if(!isFocusedOnReward){

        switch(event.keyCode) {

          case VK_UP:
          case VK_LEFT: {
            event.preventDefault();
            if( this.focusedIdx === null ) {
              this.focusedIdx = 0;
            } 
            else if (this.focusedIdx === 0) {
              this.focusedIdx = this.slots.length - 1;
            } else {
              this.focusedIdx--;
            }
            this.changeFocus(this.focusedIdx);
            break;
          }
    
          case VK_DOWN:
          case VK_RIGHT: {
            event.preventDefault();
            if( this.focusedIdx === null ) {
              this.focusedIdx = this.slots.length - 1;
            } 
            else if (this.focusedIdx === this.slots.length - 1) {
              this.focusedIdx = 0;
            } else {
              this.focusedIdx++;
            }
            this.changeFocus(this.focusedIdx);
            break;
          }
        }
      }else {
        const rewardPlaced = $(focusedElement); 
        const itemPosition = rewardPlaced.css('transform').split(',');
        const itemLeft = parseInt(itemPosition[4]);
        const itemTop = parseInt(itemPosition[5]);
        let newLeft, newTop;


        // move position of item
        switch(event.keyCode) {

          case VK_UP: {
            newLeft = itemLeft;
            newTop = itemTop - 10;
            break;
          }
          case VK_LEFT: {
            newLeft = itemLeft - 10;
            newTop = itemTop;
            break;
          }
    
          case VK_DOWN: {
            newLeft = itemLeft;
            newTop = itemTop + 10;
            break;
          }
          case VK_RIGHT: {
            newLeft = itemLeft + 10;
            newTop = itemTop;
            break;
          }
        }
        rewardPlaced.css({ transform: 'translate(' + newLeft + 'px,'+ newTop + 'px)' });
      }
    },
