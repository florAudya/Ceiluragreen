        recollect.baseURL + 'dist/images/social-twitter.svg',
        recollect.baseURL + 'dist/images/social-twitter-hover.svg',
        recollect.baseURL + 'dist/images/share-arrow.svg',
        recollect.baseURL + 'dist/images/share-arrow-hover.svg',
