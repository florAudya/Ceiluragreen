/* global $, Backbone, _, Howl, FontDetect */

(function( $ ){
  $.fn.fitTextRem = function(maxFont, minFont) {
    return this.each(function(){
      var diff = +(((maxFont - minFont) / 10).toFixed(2));
      var $this = $(this);
      $this.html('<span style="white-space: nowrap;">' + $this.html() + '</span>');

      width = $this.width();
      line = $($this.children('span'));
      var returnFontSize = maxFont;

      $this.css('font-size', returnFontSize + "rem");

      while (line.width() >= width) {
        if (returnFontSize <= minFont) {
          break;
        }
        returnFontSize = returnFontSize - diff;
        $this.css('font-size', returnFontSize + "rem");
      }
      $this.html(line.html());
      $this.css('line-height', (returnFontSize + 0.05) + "rem");
    });
  };
})( jQuery );

