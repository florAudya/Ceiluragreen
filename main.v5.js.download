    move: function(event) {
      var pageX = ['touchmove', 'touchstart'].indexOf(event.type) > -1 ? event.originalEvent.touches[0].pageX : event.originalEvent.pageX;
      var pageY = ['touchmove', 'touchstart'].indexOf(event.type) > -1 ? event.originalEvent.touches[0].pageY : event.originalEvent.pageY;
      if (debugEnv) {
        this.$('.reward-options h1').html('move: ' + event.type + ': ' + pageX + ', ' + pageY);
      }
      if (this.selected){
        var stageWidth = $(this.el).width()/100;
        var left = pageX - this.selected.width()/2 + this.selectedOffset.left;
        var top = pageY - this.selected.height()/2 + this.selectedOffset.top;
        this.selected.css({ transform: 'translate(' + (left/stageWidth)/10 + 'rem, ' + (top/stageWidth)/10 + 'rem)' + (this.selected.hasClass('flip') ? ' scaleX(-1)' : '') });
        return false;
      }
    },        
    up: function(event) {
      if (debugEnv) {
        this.$('.reward-options h1').html(event.type);
      }
      this.selected = null;
