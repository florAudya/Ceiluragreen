  var resizeTitleText = function() {
    $(".start .start-title .name").fitTextRem(0.75, 0.4);  // Make the title text fit
    $(".start .start-title .description").fitTextRem(0.35, 0.2);  // Make the description text fit
  };
  FontDetect.onFontLoaded ('Thump', resizeTitleText, resizeTitleText, {msTimeout: 3000});

  var staticHost = '/';
  var config = null;
  var level = null;
  var totalCorrect = 0;
  var currentLevel = null;
  var Sort = null;
  var Place = null;
  var preloadedSections = [];

  // We're now using px, but beware of vw on Android 4.1, Modernizr.cssvwunit is installed if you need to detect.
  var resizeTimer;
  var setStageDimensions = function() {
    clearTimeout(resizeTimer);
    resizeTimer = setTimeout(function() {
      var shortScreenRatio = window.innerHeight * (16/9) < $(window).width() ? ((window.innerHeight*(16/9))/$(window).width()) : 1;
      $('html').css('fontSize', ($(window).width()/100) * 10 * shortScreenRatio + 'px');
      window.scrollTo(0,0);
    }, 100);
  };
  setStageDimensions();
  $(window).on('resize', setStageDimensions);

  // Extract URL parameters, from http://stackoverflow.com/a/2880929
  var urlParams;
  (window.onpopstate = function () {
      var match,
          pl     = /\+/g,  // Regex for replacing addition symbol with a space
          search = /([^&=]+)=?([^&]*)/g,
          decode = function (s) { return decodeURIComponent(s.replace(pl, " ")); },
          query  = window.location.search.substring(1);

      urlParams = {};
      while (match = search.exec(query)) {
        urlParams[decode(match[1])] = decode(match[2]);
      }
  })();

  var debugEnv = typeof urlParams.debug !== 'undefined';
  var demoEnv = typeof urlParams.demo !== 'undefined';
  var appEnv = typeof urlParams.app !== 'undefined';
  var noMetricsEnv = typeof urlParams['no-metrics'] !== 'undefined' || recollect.customer_name === 'pastacity';
  var prodEnv = window.location.hostname !== 'localhost';

  if (debugEnv) {
    $('.level:not(:last-of-type)').addClass('done');
  }
  // Try to check if browser chrome visible.
  // if (Modernizr.touchevents) {
  //   $(window).on('resize', function() {
  //     $('.stage').toggleClass('tight', window.innerHeight < document.body.offsetHeight);
  //   });
  //   $('.stage').toggleClass('tight', window.innerHeight < document.body.offsetHeight);
  // }

  var fixOrientation = function() {
    switch (window.orientation) {
      case 0:
        $('.orientation-warning').removeClass('hide');
        setTimeout(function() {
          $('.orientation-warning').addClass('run-animation');
        }, 2000);
        break;
      case -90:
      case 90:
        $('.orientation-warning').addClass('hide');
        $('.orientation-warning').removeClass('run-animation');
        break;
    }
  };
  $(window).on('orientationchange', fixOrientation);

  var possibleSceneClasses = _.uniq($('[data-navigate]').map(function() { return 'scene-' + $(this).data('navigate'); })).join(' ');
  

  var muteButton = document.getElementById('mute');
  muteButton.addEventListener('click', function(event){
    var currentState = (muteButton.getAttribute('aria-pressed') == 'true');
    
    muteButton.setAttribute("aria-pressed", !currentState);
  })

  $('[data-navigate]').click(function() {
    $('.scene').removeClass('active');
    $('body').removeClass(possibleSceneClasses);
    $('body').addClass('scene-' + $(this).data('navigate'));
    $('.scene.' + $(this).data('navigate')).addClass('active');

    // Find all focusable children, focus on first in scene
    var focusableElementsString = 'button:not(.done), [tabindex="0"]';

    switch($(this).data('navigate')){
      case 'start':
        $('#playButton').focus();
        break;
      case 'sort':
        $('#materialLabel').focus();
        break;
      case 'place':
        $('#placeHeader').focus();
        break;
      default:
        $('.scene.' + $(this).data('navigate')).find(focusableElementsString).first().focus();
    }
  });

  $('.js-stat-start').click(function() {
    stat({ event: 'game_start' });
  });

  $('.level').click(function() {
    level = $(this);
    currentLevel = level.data('level');

    $('.current-level').html(level.html());
    Sort.resetScene();
  });

  $('.quit').click(function() {
    resetGame();
  });

  var resetGame = function() {
    $('.level').removeClass('done');
    $('.reward-placed .reward').remove();
    $('.reward-options').removeClass('hide');
    $('.reward-options').attr('aria-hidden', 'false');
    $('.share-sheet').addClass('hide');
    $('.share-sheet .toggle').removeClass('hide');
    $('.share-sheet').attr("aria-hidden", "true");
    $('.next-material div').remove();
    Sort.resetGame();
    totalCorrect = 0;
    $.getJSON(recollect.baseURL + 'data/config.json?' + (new Date()).getTime(), function(data) {
      config.game_id = data.game_id;
    });
  };

  function preloadImages(key, imageList) {
    if (preloadedSections.indexOf(key) === -1) {
      $(imageList).each(function () {
        $('<img>').attr('src', this);
      });
      preloadedSections.push(key);
    }
  }

  function stat(data) {
    if (prodEnv && !demoEnv && !noMetricsEnv) {
      $.ajax({
        method: 'post',
        url: recollect.apiBaseURL + 'services/waste/game/' + config.game_id + '/stats',
        data: JSON.stringify(data),
        contentType: 'application/json',
      });
    }
  }

  function renderLanguagePicker() {
    var langOptions = [];
    config.available_locales.forEach( function(locale) {
      var language = locale.name;
      var displayName = locale.title;
      if (language != recollect.lang) {
        langOptions.push({ path: language + '/', label: displayName });
      }
    });

    for (var i in langOptions) {
      var languageButton = $([
        '<a class="button smaller" lang="'+ langOptions[i].path.slice(0, -1) +'" href="' + recollect.baseURL + langOptions[i].path + '">',
          langOptions[i].label,
        '</a>'
      ].join(''));
      if (langOptions.length <= 5) {
        $('.js-language-picker-mini').append(languageButton);
      }
      else {
        $('.js-language-picker').append(languageButton);
        $('[data-navigate="locale"]').addClass('active');
      }
    }
  }

  function letThereBePeople(setStill, setWalking) {
    var kindsOfMovingPeople = 12;
    var kindsOfStillPeople = 3;
    var peoplePerReward = 2;

    // clean up
    $('.people-back').empty();
    $('.people-front').empty();

    var objectsInThePark = $('.reward-placed').children().length;

    if (objectsInThePark > 0 || typeof urlParams.people !== 'undefined' || $('.four-oh-four').length > 0 ) {
      var peopleToAdd = Math.min(Math.floor(objectsInThePark * peoplePerReward), 12);
      if (typeof urlParams.people !== 'undefined') {
        peopleToAdd = parseInt(urlParams.people, 10);
      }
      var stillPeople = setStill || Math.floor(peopleToAdd / 3);
      var walkingPeople = setWalking || peopleToAdd - stillPeople;

      // add walking people
      _.times(walkingPeople, function(n) {
        // green area range:
        var top = (Math.random() * 15 + 30);
        var left = (Math.random() * -10 -5);
        var $el = $('<div class="person"></div>');
        var classes = 'person-' + (Math.floor(Math.random() * kindsOfMovingPeople));
        if (Math.random() > 0.5) {
          classes += ' going-left';
          left = (Math.random() * 10 + 105);
        }
        $el.css({ position: 'absolute', top: top/10 + 'rem', left: left/10 + 'rem' }).addClass(classes);
        if (top > 37.5) {
          $('.people-front').append($el);
        } else {
          $('.people-back').append($el);
        }
      });
      // add still people
      _.times(stillPeople, function(n) {
        // green area range:
        var top = (Math.random() * 15 + 30);
        var left = (Math.random() * 80  + 10);
        var $el = $('<div class="person-still"></div>');
        var classes = 'person-still-' + (Math.floor(Math.random() * kindsOfStillPeople));
        if (Math.random() > 0.5) {
          classes += ' going-left';
        }
        $el.css({ position: 'absolute', top: top/10 + 'rem', left: left/10 + 'rem' }).addClass(classes);
        if (top > 37.5) {
          $('.people-front').append($el);
        } else {
          $('.people-back').append($el);
        }
      });
    }
    else {
      preloadImages('people', [
        recollect.baseURL + 'dist/images/people/person_0.svg',
        recollect.baseURL + 'dist/images/people/person_1.svg',
        recollect.baseURL + 'dist/images/people/person_2.svg',
        recollect.baseURL + 'dist/images/people/person_3.svg',
        recollect.baseURL + 'dist/images/people/person_4.svg',
        recollect.baseURL + 'dist/images/people/person_5.svg',
        recollect.baseURL + 'dist/images/people/person_6.svg',
        recollect.baseURL + 'dist/images/people/person_7.svg',
        recollect.baseURL + 'dist/images/people/person_8.svg',
        recollect.baseURL + 'dist/images/people/person_9.svg',
        recollect.baseURL + 'dist/images/people/person_10.svg',
        recollect.baseURL + 'dist/images/people/person_11.svg',
        recollect.baseURL + 'dist/images/people/still_0.svg',
        recollect.baseURL + 'dist/images/people/still_1.svg',
        recollect.baseURL + 'dist/images/people/still_2.svg',
        recollect.baseURL + 'dist/images/rewards/skater-boy.svg',
        recollect.baseURL + 'dist/images/rewards/skater-girl.svg',
      ]);
    }
  }

  function announceChange( announcement ) {
    var ariaLiveContainer = document.querySelector('[aria-live]');
    ariaLiveContainer.appendChild(document.createTextNode(announcement));
    setTimeout(function () {
      ariaLiveContainer.innerHTML = "";
    }, 2000);
  }

  var SortView = Backbone.View.extend({
    el: '.scene.sort',
    progress: 0,
    maxMaterialsPerLevel: demoEnv || debugEnv ? 1 : 6,
    currentMaterial: null,
    currentMaterialView: null,
    initialPosition: {},
    selected: null,
    selectedOffset: { top: 0, left: 0 },
    candidate: null,
    positions: null,
    focusedIdx: null,
    focusedButton: null,
    events: {
      'click .button': 'navigateToPark',
      'touchstart .material': 'down',
      'mousedown .material': 'down',
      'touchmove': 'move',
      'mousemove': 'move',
      'touchend .material': 'up',
      'mouseup .material': 'up',
      // 'mouseover .position': ''
      'keydown' : 'handleKeyPress',
      'click .position': 'handleClick',
      'focus #materialLabel': 'handleMaterialFocus',
      'focusout #materialLabel': 'handleMaterialFocusOut'
    },
    initialize: function(options) {
      var that = this;
      var imagesToPreload = [];
      this.materials = options.materials;
      this.originalMaterials = _.mapObject(this.materials, _.clone);
      options.streams.forEach(function(stream) {
        var position = null;
        
        if (RIGHT_DEPOTS.indexOf(stream.position) !== -1) {
          position = that.$('.position-right');
          position.addClass(stream.position);
          this.$('.streams').addClass('has-right');
        }
        else if (LEFT_DEPOTS.indexOf(stream.position) !== -1) {
          position = that.$('.position-left');
          position.addClass(stream.position);
          this.$('.streams').addClass('has-left');
        }
        else {
          position = $([
            '<button class="position position-' + stream.position + '">',
              '<div class="stream">',
                '<div id="' + stream.position +'Label" class="label">',
              '</div>',
            '</button>'
          ].join(''));
          this.$('.main-positions').append(position);
        }
        if (position) {
          position
            .first()
            .removeClass('hide')
            .find('.stream')
              .data('id', stream.id)
              .data('page_id', stream.page_id)
              .addClass(stream.position)
              .find('.label')
                .html(stream.label);
          var bgPath = position.find('.stream').css('backgroundImage').match(/url\(\"?([^"]*)\"?\)/);
          if (bgPath) {
            imagesToPreload.push(bgPath[1]);
          }
        }
      });
      this.$('.streams').addClass('stream-' + this.$('.main-positions .position').length);
      preloadImages('initialize-sort-view', imagesToPreload);
      this.render();
    },
    render: function() {
      _.times(this.maxMaterialsPerLevel, function(index) {
        var $progressButton = $('<div class="progress"></div>');
        this.$('.counter').append($progressButton).attr('aria-valuenow', 0).attr('aria-valuemin', 0).attr('aria-valuemax', index + 1);
      });
    },
    down: function(event) {
      var pageX = event.type === 'touchstart' ? event.originalEvent.touches[0].pageX : event.originalEvent.pageX;
      var pageY = event.type === 'touchstart' ? event.originalEvent.touches[0].pageY : event.originalEvent.pageY;
      if (debugEnv) {
        this.$('.material-label').html(event.type + ': ' + pageX + ', ' + pageY);
      }
      if (event.type === 'touchstart' || (event.type === 'mousedown' && event.which === 1)) {
        this.selected = this.currentMaterialView;
        this.selectedOffset.left = this.selected.position().left + this.selected.width()/2 - pageX;
        this.selectedOffset.top = this.selected.position().top + this.selected.height()/2 - pageY;
        this.$('.next-material').addClass('no-animate');
        this.$('.material-help').addClass('done');
        return false;
      }
      // Set material aria-grabbed true
    },
    move: function(event) {
      var pageX = event.type === 'touchmove' ? event.originalEvent.touches[0].pageX : event.originalEvent.pageX;
      var pageY = event.type === 'touchmove' ? event.originalEvent.touches[0].pageY : event.originalEvent.pageY;
      if (debugEnv) {
        this.$('.material-label').html(event.type + ': ' + pageX + ', ' + pageY);
      }
      var that = this;
      if (this.selected) {
        var stageWidth = $(this.el).width()/100;
        var left = pageX - this.selected.width()/2 + this.selectedOffset.left;
        var top = pageY - this.selected.height()/2 + this.selectedOffset.top;
        this.selected.css({ transform: 'translate(' + (left/stageWidth)/10 + 'rem, ' + (top/stageWidth)/10 + 'rem)' });

        // Update streams.
        that.candidate = null;
        this.$el.find('.stream').each(function() {
          var $stream = $(this);
          if (
            pageX > $stream.offset().left &&
            pageX < $stream.offset().left + $stream.width() &&
            pageY > $stream.offset().top &&
            pageY < $stream.offset().top + $stream.height()
          ) {
            $('.scene.sort').removeClass('show-help');
            $stream.closest('.position').addClass('over');
            that.candidate = $stream;
          }
          else {
            $stream.closest('.position').removeClass('over');
          }
        });
        return false;
      }
    },
    up: function(event) {
      if (debugEnv) {
        this.$('.material-label').html(event.type);
      }
      this.checkCandidate();
      // Set material aria-grabbed false

      this.candidate = null;
      return false;
    },
    handleMaterialFocus: function(event) {
      this.positions = $(this.el).find('.position').not('.hide').not(':disabled');
    },
    handleMaterialFocusOut: function(event) {
      $('.scene.sort').removeClass('show-help');
      this.positions = $(this.el).find('.position').not('.hide').not(':disabled');
    },
    handleClick: function(event) {
      // get stream, set this.candidate
      if($(event.target).hasClass('position')){ // if you use keyboard to 'click' button
        this.candidate = $(event.target).find('.stream');
      }else { // if you click on stream
        this.candidate = $(event.target).closest('.stream');
      }
      this.checkCandidate();

      this.candidate = null;
      return false;
    },
    resetScene: function() {
      var that = this;
      this.progress = 0;
      this.$('.button').addClass('hide');
      this.$('.counter').removeClass('dance');
      this.$('.progress').removeClass('done');

      var $shuffle = _.shuffle(this.$('.main-positions .position'));
      
      $.each($shuffle, function(i, $position) {
        that.$('.main-positions').append($position);   
        var direction = i < $shuffle.length/2 ? 'left' : 'right';
        $($position).addClass('arrow-' + direction);
      });

      if(document.getElementById("mute").getAttribute('aria-pressed') == "false"){
        startSound.play();
      }
      this.positions = $(this.el).find('.position').not('.hide');
      this.nextMaterial();
    },
    checkCandidate: function() {
      // don't allow multiple clicks
      if(!this.$el.hasClass('ready')){
        return;
      }

      if (
        this.candidate &&
        (
          this.candidate.data('id') === this.currentMaterial.stream || // String with single stream.
          (typeof this.currentMaterial.stream !== 'string' && this.currentMaterial.stream.indexOf(this.candidate.data('id')) > -1) || // Array of streams.
          debugEnv
        )
      ) {
        stat({
          event: 'correct_guess',
          material_page_id: this.currentMaterial.page_id,
          stream_page_id: this.candidate.data('page_id'),
        });
        this.candidate.closest('.position').addClass('over');
        this.candidate.addClass('correct');
        announceChange("Correct");
        this.correctAnswer();
        this.selected = null;

      }
      else {
        if (this.candidate) {
          if (!this.candidate.hasClass('wrong')) {
            if(document.getElementById("mute").getAttribute('aria-pressed') == "false"){
              if (this.candidate.hasClass('depot') || this.candidate.hasClass('return')) {
                wrongDepotSound.play();
              }
              else {
                wrongBinSound.play();
              }
            }
            this.candidate.closest('.position').removeClass('over');
            this.candidate.addClass('wrong');
            this.candidate.parent()[0].setAttribute('disabled', ''); // set button to disabled

            setTimeout( function(){
              announceChange("Try again");
              $('#materialLabel').focus();
            }, 500)
            stat({
              event: 'incorrect_guess',
              material_page_id: this.currentMaterial.page_id,
              stream_page_id: this.candidate.data('page_id'),
            });
          }
        }
        this.$el.find('.next-material').removeClass('no-animate');
        if (this.selected) {
          this.selected.css({ transform: 'translate(' + this.initialPosition.left/10 + 'rem, ' + this.initialPosition.top/10 + 'rem)' });
          this.selected = null;
        }
      }
      this.focusedIdx = null;

    },
    changeFocus : function(index) {
      this.focusedButton = this.positions[index];
      this.focusedButton.focus();
    },
    handleKeyPress: function(event){
      var VK_LEFT       = 37;
      var VK_UP         = 38;
      var VK_RIGHT      = 39;
      var VK_DOWN       = 40;

      switch(event.keyCode) {

        case VK_UP:
        case VK_LEFT: {
          event.preventDefault();
          if( this.focusedIdx === null ) {
            this.focusedIdx = 0;
          } 
          else if (this.focusedIdx === 0) {
            this.focusedIdx = this.positions.length - 1;
          } else {
            this.focusedIdx--;
          }
          this.changeFocus(this.focusedIdx);
          break;
        }
  
        case VK_DOWN:
        case VK_RIGHT: {
          event.preventDefault();
          if( this.focusedIdx === null ) {
            this.focusedIdx = this.positions.length - 1;
          } 
          else if (this.focusedIdx === this.positions.length - 1) {
            this.focusedIdx = 0;
          } else {
            this.focusedIdx++;
          }
          this.changeFocus(this.focusedIdx);
          break;
        }
      }
    },
    resetGame: function() {
      this.materials['1'] = _.map(this.originalMaterials['1'], _.clone);
      this.materials['2'] = _.map(this.originalMaterials['2'], _.clone);
      this.materials['3'] = _.map(this.originalMaterials['3'], _.clone);
    },
    correctAnswer: function() {
      this.$el.removeClass('ready'); // prevent multiple clicks
      var that = this;
      this.currentMaterialView.remove();
      this.progress += 1;
      totalCorrect += 1;
      if(document.getElementById("mute").getAttribute('aria-pressed')  == "false"){
        correctSound.play();
      }
      this.$('.progress:not(.done)').first().addClass('done');
      this.$('.position').removeAttr('disabled');
      this.$('.counter').attr('aria-valuenow', this.progress);
      this.$('.position').removeClass('over');

      var top = this.candidate.offset().top + this.candidate.height()/2 - $('.stage').offset().top;
      var left = this.candidate.offset().left + this.candidate.width()/2 - $('.stage').offset().left;
      _.times(20, function(n) {
        var $el = $('<div class="star"></div>');
        var classes = 'star-' + n;
        if (Math.random() > 0.5) {
          classes += ' small';
        }
        $el.css({ position: 'absolute', top: top, left: left }).addClass(classes);
        this.$('.stars').append($el);
      });
      that.$('.material-label').addClass('hide');
      setTimeout(function() {
        that.$('.stars .star').remove();
        if (that.progress < that.maxMaterialsPerLevel) {
          that.nextMaterial();
          setTimeout( function(){
            $('#materialLabel').focus();
          }, 500)
        }
        else {
          level.addClass('done'); // We're done this level.
          that.$('.material-label').html(translation.success).removeClass('hide').addClass('success-text');
          that.$('.counter').addClass('dance');

          announceChange(translation.success);
          setTimeout( function(){
            that.$('.button').removeClass('hide');
            that.$('.button').focus();
          }, 1500)
          this.positions = null;
          if(document.getElementById("mute").getAttribute('aria-pressed')  == "false"){
            yaySound.play();
          }
          // All levels done, star fireworks show!
          if ($('.level:not(.done)').length <= 0) {
            this.$('.position').each(function(index) {
              var $item = $(this);
              setTimeout(function() {
                var top = $item.offset().top + $item.height()/2 - $('.stage').offset().top;
                var left = $item.offset().left + $item.width()/2 - $('.stage').offset().left;
                _.times(20, function(n) {
                  var $el = $('<div class="star"></div>');
                  var classes = 'star-' + n;
                  if (Math.random() > 0.5) {
                    classes += ' small';
                  }
                  $el.css({ position: 'absolute', top: top, left: left }).addClass(classes);
                  this.$('.stars').append($el);
                });
              }, 250 * index);
            });
            setTimeout(function() {
              that.$('.stars .star').remove();
            }, 250 * this.$('.position').length + 1000);
          }
          this.progress = 0; //reset progress bar
        }
      }, 1250);
    },
    nextMaterial: function() {
      preloadImages('during-play', [
        recollect.baseURL + 'dist/images/social-email.svg',
        recollect.baseURL + 'dist/images/social-email-hover.svg',
        recollect.baseURL + 'dist/images/social-facebook.svg',
