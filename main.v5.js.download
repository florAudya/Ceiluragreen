      Place.chooseOptions();
      // [data-navigate] changes scene.
    },
  });

  var PlaceView = Backbone.View.extend({
    el: '.scene.place',
    objectTemplate: _.template($('#reward').html()),
    maxObjects: 6,
    previous: null,
    selected: null,
    selectedOffset: { top: 0, left: 0 },
    rewards: null,
    customRewards: null,
    secondClick: false,
    slots: null,
    focusedIdx: null,
    focusedButton: null,
    events: {
      'click .continue': 'continueButton',
      'click .finish': 'finishButton',
      'click .play-again': 'playAgainButton',
      'click .pull-cord': 'pullCord',
      'click .print': 'print',
      'click .print-done': 'printDone',
      'click .contest-done': 'contestDone',
      'touchstart .reward-options .reward': 'choose',
      'mousedown .reward-options .reward': 'choose',
      'touchstart .reward': 'pickup',
      'mousedown .reward': 'pickup',
      'touchmove': 'move',
      'mousemove': 'move',
      'touchend': 'up',
      'mouseup': 'up',
      'click .slot': 'choose',
      'keydown' : 'handleKeyPress',
      'focusout #placeHeader': 'handleHeadingFocusOut'
